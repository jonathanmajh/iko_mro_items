{
  "name": "iko_utility",
  "productName": "EAM Spare Parts",
  "version": "3.8.8",
  "description": "Tool to help automate various Reliability Processes",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": "https://github.com/jonathanmajh/iko_mro_items",
  "keywords": [],
  "author": {
    "name": "Jonathan Ma",
    "email": "jonathanmajh@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/rebuild": "^4.0.1",
    "electron": "^38.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9.39.1",
    "eslint-config-google": "^0.14.0",
    "npm-check-updates": "^19.1.2"
  },
  "dependencies": {
    "@zip.js/zip.js": "^2.8.14",
    "better-sqlite3": "^12.4.1",
    "bootstrap": "^5.3.8",
    "electron-squirrel-startup": "^1.0.1",
    "exceljs": "^4.4.0",
    "firebase": "^12.5.0",
    "jimp": "^1.6.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "AppIcon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "iko_utility",
            "setupIcon": "AppIcon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "jonathanmajh",
              "name": "iko_mro_items"
            },
            "draft": false,
            "prerelease": false,
            "generateReleaseNotes": true
          }
        }
      ]
    }
  }
}
